{
  "name": "ui-kit-bundler",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/ui-kit-bundler.umd.js",
  "module": "./dist/ui-kit-bundler.esm.js",
  "unpkg": "./dist/ui-kit-bundler.min.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "webpack serve --mode development",
    "pack": "npm pack --pack-destination=./library/",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/ui-kit-bundler.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/ui-kit-bundler.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/ui-kit-bundler.min.js",
    "build:style": "sass --no-charset ./src/styles/index.scss | node ./build/print-banner.js > dist/ui-kit-bundler.css && cleancss -o dist/ui-kit-bundler.min.css dist/ui-kit-bundler.css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "filemanager-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.27",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.0.0",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.1.0",
    "rollup": "^1.17.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-typescript2": "^0.35.0",
    "rollup-plugin-vue": "^5.0.1",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "style-resources-loader": "^1.5.0",
    "ts-loader": "^9.4.4",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vue": "^2.6.14",
    "vue-loader": "^15.10.1",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
